import sys
from time import time

t = time()
hg = list[str]()
for l in sys.stdin:
	if l[0] != '>':
		hg.append(''.join(['A' if c != 'A' and c != 'C' and c != 'G' and c != 'T' else c for c in l]))
s = ''.join(hg) #str.cat(hg)
sys.stderr.write(str((time()-t)/1000.0) + '\n')

for i in range(0, len(s), 100):
	w = s[i:i+100]
	print '@' + str(i)
	print w
	print '+'
	print 'A' * len(w)
